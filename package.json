{
  "name": "my-electron-template",
  "productName": "my-electron-template",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "pg:migration:generate": "drizzle-kit generate:pg --config drizzle.config.pg.ts",
    "pg:migration:up": "drizzle-kit up:pg --config drizzle.config.pg.ts",
    "pg:migration:check": "drizzle-kit up:pg --config drizzle.config.pg.ts",
    "pg:migration:generate:custom": "drizzle-kit generate:pg --custom --config drizzle.config.pg.ts",
    "pg:migration:drop": "drizzle-kit drop --config drizzle.config.pg.ts",
    "pg:migration:deploy": "tsx src/deploy.ts",
    "pg:push": "drizzle-kit push:pg --config drizzle.config.pg.ts",
    "pg:introspect": "drizzle-kit introspect:pg --config drizzle.config.pg.ts",
    "pg:studio": "drizzle-kit studio --config drizzle.config.pg.ts --host 127.0.0.1",
    "sqlite:push": "drizzle-kit push:sqlite --config drizzle.config.sqlite.ts",
    "sqlite:generate": "npx drizzle-kit generate --config=drizzle.config.sqlite.ts",
    "sqlite:generate:custom": "drizzle-kit generate --custom --config drizzle.config.sqlite.ts",
    "sqlite:migration:deploy": "tsx src/lib/db/sqlite/deploy.sqlite.ts",
    "sqlite:studio": "drizzle-kit studio --config drizzle.config.sqlite.ts --host 127.0.0.1"
  },
